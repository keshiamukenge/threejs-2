bject => {\n            object.link.addEventListener('mousemove', (e) => {\n                console.log(object.mesh.mesh.material.uniforms.uOffset.value)\n                console.log(e)\n                GSAP.to(object.mesh.mesh.position, {\n                    x: (e.clientX - object.mesh.mesh.position.x) * 0.5,\n                    y: - e.clientY - object.mesh.mesh.position.y\n                });\n                GSAP.to(object.mesh.mesh.material.uniforms.uOffset.value, {\n                    x: e.clientX,\n                    y: - e.clientY\n                });\n            });\n        });\n    }\n}\n\nnew App();"},"lineCount":null}},"hash":"36a95f515bf9b92f6dc868b7b830624d","cacheData":{"env":{}},"id":6}